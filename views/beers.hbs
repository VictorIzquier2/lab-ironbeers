
<div class="container-fluid">
    {{#each beers}}
  <div class="col-md-4">
    <img class="card-img-top" src="{{this.image_url}}" alt="{{this.name}}"/>
    <div class="card-body">
      <h5 class="card-title">{{this.name}}</h5>
      <h5 class="card-text">{{this.description}}</h5>
      <h5 class="card-text">{{this.tagline}}</h5>
    </div>
  </div>
    {{/each}}
</div>